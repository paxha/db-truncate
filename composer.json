{
    "name": "paxha/db-truncate",
    "description": "Truncate all db tables",
    "type": "laravel",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "paxha/db-truncate": "master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/paxha/db-truncate"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Paxha\\DBTruncate\\DBTruncateServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Paxha\\DBTruncate\\": "src/"
        }
    }
}
